{
  "name": "heuvera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "generate:swagger": "bun scripts/swagger.ts",
    "generate:client": "bunx swagger-typescript-api generate -p public/swagger.json -o src/client",
    "build": "bun run generate:swagger && bun run prettier:write && bun run generate:client && prisma generate && next build",
    "postinstall": "bun run prettier:write && bun run generate:swagger && bun run generate:client && prisma generate",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit"
  },
